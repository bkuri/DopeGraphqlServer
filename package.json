{
  "name": "dope-graphql-server",
  "version": "0.0.2",
  "description": "Dope GraphQL Server",
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon src/server.js --exec babel-node",
    "flow": "./node_modules/.bin/flow check ./src",
    "build": "./node_modules/.bin/babel src -d dist --copy-files --source-maps=true",
    "flow-watch": "babel --watch=./src --out-dir=./dist --copy-files --source-maps=true",
    "lint": "node_modules/eslint/bin/eslint.js src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "graphql": "^0.9.1",
    "graphql-server-express": "^0.6.0",
    "graphql-tools": "^0.6.4",
    "iterall": "^1.0.3",
    "lodash": "^4.15.0",
    "request": "^2.74.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-flowtype": "^2.11.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "flow-bin": "^0.30.0",
    "nodemon": "^1.10.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    }
  },
  "keywords": [
    "GraphQL"
  ],
  "author": "Bernardo Kuri",
  "license": "ISC",
  "homepage": "http://bkuri.com",
  "repository": "http://bkuri.com",
  "bugs": "bugs@bkuri.com",
  "contributors": [],
  "engines": {}
}
